{
    "manifest_version": 3,
    "name": "Quoter",
    "version": "1.0",
    "description": "quoting in gmail easier",
    "action":{
      "default_icon": "images/quotes-small.png"
    },
    "web_accessible_resources":    [ {
      "resources": [ "images/quotes-small.png" ],
      "matches": ["<all_urls>"]
    }],
    "content_scripts": [
      {
        "js": ["scripts/content.js"],
        "matches": [
          "https://mail.google.com/*"
        ]
      }
    ]
  }